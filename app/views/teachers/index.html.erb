<p id="notice"><%= notice %></p>

<h1>Teachers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <%# <th>Student</th> %>
      <th>Strong sub</th>
      <th>Strong sub level</th>
      <th>Weak sub</th>
      <th>Weak sub level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.name %></td>
        <%# <td><%= teacher.student_id %>
        <td><%= teacher.strong_sub %></td>
        <td><%= teacher.strong_sub_level %></td>
        <td><%= teacher.weak_sub %></td>
        <td><%= teacher.weak_sub_level %></td>
        <td><%= link_to 'Show', teacher %></td>
        <td><%= link_to 'Edit', edit_teacher_path(teacher) %></td>
        <td><%= link_to 'Destroy', teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher', new_teacher_path %>
